# Copy to .env and fill in your values

# Cloudinary Configuration
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
VITE_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# API Configuration - Update this with your Render backend URL
VITE_API_BASE_URL=https://your-backend-app.onrender.com/api/v1

# Site Configuration - Update this with your Vercel frontend URL
VITE_PUBLIC_SITE_URL=https://your-app.vercel.app

# Optional: Analytics/Monitoring
VITE_GA_TRACKING_ID=
VITE_SENTRY_DSN=
